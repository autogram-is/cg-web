<div class="region region-double">
  <div class="gallery {{ utilities }}">
    <div class="wrapper gallery-inner">
      {% for image in images -%}
      <img 
        src="{{ get_image(image) }}"
        srcset="{{ get_image(image).srcset }}"
        sizes="auto"
        height="{{ get_image(image).height }}"
        width="{{ get_image(image).width }}"
        loading="lazy">
      {%- endfor %}
    </div>
  </div>
</div>
