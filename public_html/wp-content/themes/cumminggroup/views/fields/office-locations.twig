{% for office in city.office_details %}
<div class="lede-header office region">

  {% if office.address %}
  <p class="icon-map">
    <a href="{{ office.address | map_link }}">{{ office.address }}</a>
  </p>
  {% endif %}
  {% if office.phone and (office.phone | phone) %}<p class="icon-tel"><a href="{{ office.phone | phone_url }}">{{ office.phone | phone }}</a></p>{% endif %}

      {% if office.name %}
  <div class="office-name">
    <small class="note"><strong>{{ office.name }}</strong> A Cumming Group company</small>
  </div>
  {% endif %}
</div>
{% endfor %}
