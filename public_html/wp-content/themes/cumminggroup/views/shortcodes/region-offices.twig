<div class="location flow">
  {% if heading %}<h2 class="type-eyebrow hed-accent">{{ heading }}</h2>{% endif %}
  <ul class="flow" role="list">
  {% for post in posts %}
    <li class="hed type-hed icon-map">
      <a href="{{ post.link }}">{{ post.title }}</a>
    </li>
  {% endfor %}
  </ul>
</div>
